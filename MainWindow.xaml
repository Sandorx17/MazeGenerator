<Window x:Class="MazeGenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MazeGenerator"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="800" Background="#FF1F1F1F">
    <Grid Width="800" RenderTransformOrigin="0.5,0.5" Height="790" VerticalAlignment="Top" Background="#FF1F1F1F">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="331*"/>
            <ColumnDefinition Width="121*"/>
            <ColumnDefinition Width="348*"/>
        </Grid.ColumnDefinitions>
        <Canvas x:Name="Kanvasz" SnapsToDevicePixels="True" Grid.ColumnSpan="3" Margin="50,50,50,50" MinWidth="500" MinHeight="500" Width="700" Height="690" Visibility="Visible"/>
        <Button x:Name="Gomb" Click="Gomb_Click" Content="Randomizator" Grid.Column="2" HorizontalAlignment="Left" Margin="220,750,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
